{
    "manifest_version": 3,
    "name": "Fact Check Extension",
    "description": "Extension",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "/images/icon.png",
        "32": "/images/icon.png",
        "48": "/images/icon.png",
        "128": "/images/icon.png"
      }
    }
  }